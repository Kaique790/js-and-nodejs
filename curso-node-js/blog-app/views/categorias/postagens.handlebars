<h2>{{categoria.nome}}</h2>
<div class="card mt-4">
{{#each postagens}}

    <div class="card-body">
        <h4>Titulo: {{titulo}}</h4>
        <p>Descrição: {{descricao}}</p>
        <a href="/postagens/{{slug}}">
        </a>
        <hr>
    </div>
    
{{else}}
 <h4> Nenhuma postagem</h4>
{{/each}}
</div>